package com.test01;

import java.util.Scanner;

public class ExceptionCal {

	public int calculation() {
		int res = 0;
		
		while (true) {
		res = inputNum() / inputNum();
		break;
		}
		return res;
	}
	
	public int inputNum() {
		int n = 0;
		System.out.println("숫자만 입력하세요.");
		Scanner sc = new Scanner(System.in);
		n = sc.nextInt();
		
		return n;
	}
}

