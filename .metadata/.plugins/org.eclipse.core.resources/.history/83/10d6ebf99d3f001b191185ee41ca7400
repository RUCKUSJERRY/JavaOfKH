package com.test01;

public class Hello {

	public static void main(String[] args) {
	
		System.out.println("Hello, World!");

		int a=1;

		  a++;

		  System.out.println(a);

		  --a;

		  System.out.println(a);

		  int a1=a++;

		  System.out.println(a1);

		  int a2=a--;

		  System.out.println(a2);

		  int a3=++a; 

		  System.out.println(a3);

		  int a4=--a;

		  System.out.println(a4); 
		
	}
}
