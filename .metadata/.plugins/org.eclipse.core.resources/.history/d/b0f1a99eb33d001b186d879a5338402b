package com.generic;

public class MTest {

	public static void main(String[] args) {
		
		Emp emp = new Emp();
		emp.setEmpno(111);
		emp.setEname("홍길동");
		System.out.println(emp.getEname() + ":" + emp.getEmpno());
		
		Emp emp2 = new Emp("112a", "김선달");
		System.out.println(emp2.getEname() + ":" + emp2.getEmpno());
		
	}
}
